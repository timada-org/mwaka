{
  "name": "mwaka",
  "version": "0.0.8",
  "description": "A collection of libraries and tools that help you build adaptive, accessible, and robust user experiences.",
  "files": [
    "dist"
  ],
  "main": "dist/components/esm/index.js",
  "module": "dist/components/esm/index.js",
  "types": "dist/components/types/index.d.ts",
  "exports": {
    "./aria": {
      "solid": "./dist/aria/source/index.jsx",
      "import": "./dist/aria/esm/index.js",
      "browser": "./dist/aria/esm/index.js",
      "require": "./dist/aria/esm/index.js",
      "node": "./dist/aria/esm/index.js"
    },
    ".": {
      "solid": "./dist/components/source/index.jsx",
      "import": "./dist/components/esm/index.js",
      "browser": "./dist/components/esm/index.js",
      "require": "./dist/components/esm/index.js",
      "node": "./dist/components/esm/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "aria": [
        "dist/aria/types/index.d.ts"
      ]
    }
  },
  "scripts": {
    "postpublish": "npm update mwaka -w demo -w components"
  },
  "peerDependencies": {
    "solid-js": "^1.3.14"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timada-org/mwaka.git"
  },
  "keywords": [],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/timada-org/mwaka/issues"
  },
  "homepage": "https://github.com/timada-org/mwaka#readme"
}
